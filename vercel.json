{
    "rewrites": [ {"source": "/(.*)", "destination": "/"} ] ,
    "builds":[
        {
            "src":"backends/wsgi.py",
            "use":"@vercel/python",
            "congig":{
                "maxLambdaSize":"15mb",
                "runtime":"python3.11"
            }
        }
    ],
   
    "routes":[
        {
            "src":"/(.*)",
            "dest":"backends/backends/wsgi.py"
        }
    ]
}